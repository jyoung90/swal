<div class="container">
  <div class="row">
    <div class="col-lg-6">
      <h1>Post List</h1>
      <%= link_to 'New', new_post_path %>
      <hr>
      <% @posts.each do |post| %>
        <div class="well">
          <%= post.title %></br>
          <%= post.description %>
        </div>
      <% end %>
    </div>
  </div>
</div>
